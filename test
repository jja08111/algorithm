#include <iostream>
#include <cstring>
using namespace std;

double cache[50][100];
bool adj[50][50];
int day,prison;

void init(void)
{
    int n;
    
    cin>>n>>day>>prison;
    
    for(int i=0;i<n;++i)
    {
        memset(cache[i],-1,sizeof(double)*100);
        for(int j=0;j<n;++j)
            scanf("%d",&adj[i][j]);
    }
}

int main(void)
{
    int testCase;
    
    cin>>testCase;
    
    while(testCase--)
    {
        init();
        
        cout<<poly(n,0)<<endl;
    }
    
    return 0;
}
